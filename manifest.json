{
    "manifest_version": 2,
    "name": "Redify",
    "description": "this extension turns background colors red",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": ""
    },
    "content_scripts": [
        {
            "matches": ["*://*.com/*"],
            "js": ["script.js"]
        }
    ]
}