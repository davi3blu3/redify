{
    "manifest_version": 1,
    "name": "Redify",
    "description": "this extension turns background colors red",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}